{
  "name": "rxnn",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stayradiated/rxnn.git"
  },
  "homepage": "https://github.com/stayradiated/rxnn",
  "bugs": {
    "url": "https://github.com/stayradiated/rxnn/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tidy": "biome check --fix --unsafe . && eslint --fix .",
    "knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.5",
    "@sveltejs/kit": "2.22.1",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "@types/better-sqlite3": "7.6.13",
    "eslint": "9.29.0",
    "eslint-plugin-svelte": "3.9.3",
    "knip": "5.61.2",
    "svelte": "5.34.8",
    "svelte-check": "4.2.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0",
    "vite": "6.3.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  },
  "dependencies": {
    "@fast-csv/format": "5.0.2",
    "@oslojs/crypto": "1.0.1",
    "@oslojs/encoding": "1.1.0",
    "@sveltejs/adapter-node": "5.2.12",
    "better-sqlite3": "12.1.1",
    "isomorphic-dompurify": "2.25.0",
    "marked": "16.0.0",
    "normalize.css": "8.0.1",
    "zod": "3.25.67",
    "zod-form-data": "3.0.0"
  },
  "knip": {
    "svelte": {
      "entry": [
        "./src/routes/**/+{layout,page,server,error}{,.server}.{svelte,ts}",
        "./src/hooks.client.ts",
        "./src/hooks.server.ts",
        "./src/**/*.test.ts"
      ],
      "project": [
        "./src/**/*.ts"
      ]
    },
    "paths": {
      "$env/*": [
        "./node_modules/@sveltejs/kit/src/runtime/env/*"
      ]
    },
    "ignore": [],
    "ignoreDependencies": [],
    "ignoreBinaries": [],
    "ignoreExportsUsedInFile": true
  }
}
